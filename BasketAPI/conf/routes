# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET       /basket/:userId           controllers.BasketController.getBasket(userId: String)
OPTIONS   /basket/:userId           controllers.BasketController.preflightCreateBasket(userId: String)
POST      /basket/:userId           controllers.BasketController.createBasket(request: Request, userId: String)
